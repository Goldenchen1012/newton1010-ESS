N/* Private define ------------------------------------------------------------*/
N/* Private macro -------------------------------------------------------------*/
N/* Private variables ---------------------------------------------------------*/
N/* Private function prototypes -----------------------------------------------*/
N/* Public function prototypes -----------------------------------------------*/
Nint32_t doCalibration(tCalibCoef *par, int32_t dataX){ //Y = A1*X + A2*X/10000 + B
N	int32_t dataTemp;
N	dataTemp = dataX * par->A1;
N	dataTemp += (dataX * par->A2) / 100000;
N	dataTemp += par->B;
N	return dataTemp;
N}
N
NtCalibCoef calCoef(int32_t valL, int32_t adcL, int32_t valH, int32_t adcH){
N    tCalibCoef coef;
N    double y1, y2, x1, x2;
N    double cA, cB;
N    
N    if(valL == valH){
N        coef.A1 = 0;
N        coef.A2 = 0;
N        coef.B = 0;
N    }else{
N        y1 = valL;
N        y2 = valH;
N        x1 = adcL;
N        x2 = adcH;
N        cA = (y1 - y2) / (x1 - x2);
N        cB = y1 - (cA * x1);
N        coef.A1 = (int32_t)cA;
N        coef.A2 = (int32_t)((cA - (double)coef.A1) * 100000);
N        coef.B = (int32_t)cB;
N    }
N#if 0    
S    {
S    	char	str[100];
S    	sprintf(str,"A1=%d A2=%d B =%d",
S    			coef.A1,	coef.A2,coef.B);
S    		
S    	appSerialCanDavinciSendTextMessage(str);
S    }
N#endif    
N    return coef;
N}
